{
  "hosting": {
    "public": "web-app/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/pledge",
        "run": {
          "serviceId": "pledge",
          "region": "us-central1"
        }
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}
